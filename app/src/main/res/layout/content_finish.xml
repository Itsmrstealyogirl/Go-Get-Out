<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_orange_light"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.kshit.go_get_out.FinishActivity"
    tools:showIn="@layout/activity_finish">

    <EditText
        android:id="@+id/editText3"
        android:layout_width="65dp"
        android:layout_height="39dp"
        android:background="@android:color/holo_purple"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="207dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="184dp"
        android:layout_height="42dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Return to Main Menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.944" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="168dp"
        android:layout_height="29dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:background="@android:color/darker_gray"
        android:text="YOU'RE FINISHED!!"
        android:textAlignment="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="24dp"
        app:layout_constraintHorizontal_bias="0.46" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="64dp"
        android:layout_height="42dp"
        android:background="@android:color/holo_orange_dark"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="100dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="104dp"
        android:layout_height="28dp"
        android:text="MILES"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="114dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="65dp"
        android:layout_height="39dp"
        android:background="@android:color/holo_red_light"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="303dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MINUTES"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="216dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MPH"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="313dp" />

    <TextView
        android:id="@+id/howtextView5"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HOW ARE YOU FEELING?"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="361dp" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="164dp"
        android:layout_height="22dp"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="396dp" />

    <TextView
        android:id="@+id/tiredtextView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        android:background="@android:color/darker_gray"
        android:text="TIRED"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="396dp" />

    <TextView
        android:id="@+id/awsmtextView7"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_blue_bright"
        android:text="AWESOME"
        tools:layout_editor_absoluteX="300dp"
        tools:layout_editor_absoluteY="396dp" />

</android.support.constraint.ConstraintLayout>
